local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()



local Window = Library.CreateLib("Anime Race Clicker by Nv", "DarkTheme")



local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")

Section:NewToggle("autoTap", "", function(state)
_G.autoTap = state

if state then
task.spawn(function()
         while _G.autoTap == true do
             game:GetService("ReplicatedStorage").Knit.Services.ClickerService.RF.PlayerClick:InvokeServer()
            wait()
        end
    end)
end
end)


local Section = Tab:NewSection("AutoHatch  (must be next to the star)")
Section:NewToggle("Naruto1", "", function(state)
_G.autoEgg = state
task.spawn(function()
    while wait() do
        if not _G.autoEgg then break end;
       
         local args = {
         [1] = "Star1",
            [2] = 1,
            [3] = {}}

            game:GetService("ReplicatedStorage").Knit.Services.StarEggService.RF.OpenEggs:InvokeServer(unpack(args))
        end
    end)
end)

